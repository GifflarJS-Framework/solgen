[
  {
    "name": "DHT11",
    "contract": {
      "variables": [
        {
          "statement": "global_variable",
          "type": "address",
          "name": "manager",
          "scope": "public",
          "value": "",
          "setMethod": false
        },
        {
          "statement": "global_variable",
          "type": "string",
          "name": "name",
          "scope": "public",
          "value": "",
          "setMethod": true
        },
        {
          "statement": "global_variable",
          "type": "uint256",
          "name": "value1",
          "scope": "public",
          "value": "",
          "setMethod": false
        },
        {
          "statement": "global_variable",
          "type": "uint256",
          "name": "max_value1",
          "scope": "public",
          "value": "",
          "setMethod": false
        },
        {
          "statement": "global_variable",
          "type": "uint256",
          "name": "min_value1",
          "scope": "public",
          "value": "",
          "setMethod": false
        }
      ],
      "functions": [
        {
          "name": "",
          "scope": "public",
          "isConstructor": true,
          "inputs": [
            {
              "name": "_owner",
              "type": "address"
            }
          ],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "assignment",
              "variable": "manager",
              "value": { "statement": "expression", "value": "_owner" }
            },
            {
              "statement": "assignment",
              "variable": "name",
              "value": { "statement": "expression", "value": "\"DHT11\"" }
            }
          ]
        },
        {
          "name": "setValue",
          "scope": "public",
          "isConstructor": false,
          "inputs": [
            {
              "name": "_val",
              "type": "uint256"
            },
            {
              "name": "_valueId",
              "type": "uint256"
            }
          ],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "if",
              "else": false,
              "condition": "_valueId == 1",
              "content": [
                {
                  "statement": "assignment",
                  "variable": "value1",
                  "value": { "statement": "expression", "value": "_val" }
                },
                {
                  "statement": "if",
                  "else": false,
                  "condition": "value1 >= max_value1",
                  "content": [
                    {
                      "statement": "event_call",
                      "name": "temperatureOverflow",
                      "inputs": [
                        {
                          "name": "value1",
                          "type": "uint256"
                        },
                        {
                          "name": "max_value1",
                          "type": "uint256"
                        }
                      ]
                    }
                  ]
                },
                {
                  "statement": "if",
                  "else": true,
                  "condition": "value1 <= min_value1",
                  "content": [
                    {
                      "statement": "event_call",
                      "name": "temperatureUnderflow",
                      "inputs": [
                        {
                          "name": "value1",
                          "type": "uint256"
                        },
                        {
                          "name": "min_value1",
                          "type": "uint256"
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  },
  {
    "name": "Controller",
    "contract": {
      "variables": [
        {
          "statement": "global_variable",
          "type": "DHT11[]",
          "name": "contracts",
          "scope": "public",
          "value": "",
          "setMethod": false
        },
        {
          "statement": "global_variable",
          "type": "uint256",
          "name": "counter",
          "scope": "private",
          "value": "0",
          "setMethod": false
        }
      ],
      "functions": [
        {
          "name": "createContract",
          "scope": "public",
          "isConstructor": false,
          "inputs": [
            {
              "name": "_owner",
              "type": "address"
            }
          ],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "variable",
              "type": "DHT11",
              "name": "newContract",
              "value": {
                "statement": "newcontract",
                "contractName": "DHT11",
                "args": ["_owner"]
              }
            },
            {
              "statement": "method_call",
              "variable": "contracts",
              "method": "push",
              "value": "newContract"
            },
            {
              "statement": "assignment",
              "variable": "counter",
              "value": { "statement": "expression", "value": "counter + 1" }
            }
          ]
        },
        {
          "name": "getLastContract",
          "scope": "public",
          "isConstructor": false,
          "inputs": [],
          "outputs": ["_contract"],
          "modifiers": [],
          "content": [
            {
              "statement": "variable",
              "type": "DHT11",
              "name": "_contract",
              "value": ""
            },
            {
              "statement": "if",
              "else": false,
              "condition": "counter > 0",
              "content": [
                {
                  "statement": "assignment",
                  "variable": "_contract",
                  "value": {
                    "statement": "expression",
                    "value": "contracts[counter - 1]"
                  }
                }
              ]
            },
            {
              "statement": "if",
              "else": true,
              "condition": "",
              "content": [
                {
                  "statement": "assignment",
                  "variable": "_contract",
                  "value": {
                    "statement": "expression",
                    "value": "contracts[0]"
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
]
