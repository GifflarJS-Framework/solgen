[
  {
    "contract": {
      "name": "DHT11",
      "inherits": [],
      "usings": [],
      "structs": [],
      "variables": [
        {
          "type": "address",
          "name": "manager",
          "scope": "public"
        },
        {
          "type": "string",
          "name": "name",
          "scope": "public"
        },
        {
          "type": "uint256",
          "name": "value1",
          "scope": "public"
        },
        {
          "type": "address[][]",
          "name": "value2",
          "scope": "public"
        },
        {
          "type": "uint256",
          "name": "max_value1",
          "scope": "public"
        },
        {
          "type": "uint256",
          "name": "min_value1",
          "scope": "public"
        }
      ],
      "mappings": [],
      "events": [
        {
          "name": "temperatureOverflow",
          "inputs": [
            { "name": "value1", "type": "uint256" },
            { "name": "max_value1", "type": "uint256" }
          ]
        },
        {
          "name": "temperatureUnderflow",
          "inputs": [
            { "name": "value1", "type": "uint256" },
            { "name": "min_value1", "type": "uint256" }
          ]
        }
      ],
      "modifiers": [],
      "customErrors": [],
      "functions": [
        {
          "name": "",
          "scope": "public",
          "isConstructor": true,
          "inputs": [
            {
              "name": "_owner",
              "type": "address"
            }
          ],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "assignment",
              "variable": "manager",
              "expressionValue": {
                "customExpression": "_owner"
              }
            },
            {
              "statement": "assignment",
              "variable": "name",
              "expressionValue": {
                "customExpression": "\"DHT11\""
              }
            }
          ]
        },
        {
          "name": "setValue",
          "scope": "public",
          "isConstructor": false,
          "inputs": [
            {
              "name": "_val",
              "type": "uint256"
            },
            {
              "name": "_valueId",
              "type": "uint256"
            }
          ],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "if",
              "else": false,
              "condition": "_valueId == 1",
              "content": [
                {
                  "statement": "assignment",
                  "variable": "value1",
                  "expressionValue": {
                    "customExpression": "_val"
                  }
                },
                {
                  "statement": "if",
                  "else": false,
                  "condition": "value1 >= max_value1",
                  "content": [
                    {
                      "statement": "event_call",
                      "name": "temperatureOverflow",
                      "variables": ["value1", "max_value1"]
                    }
                  ]
                },
                {
                  "statement": "if",
                  "else": true,
                  "condition": "value1 <= min_value1",
                  "content": [
                    {
                      "statement": "event_call",
                      "name": "temperatureUnderflow",
                      "variables": ["value1", "min_value1"]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "setName",
          "scope": "public",
          "isConstructor": false,
          "inputs": [{ "name": "_name", "type": "string" }],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "assignment",
              "variable": "name",
              "expressionValue": {
                "customExpression": "_name"
              }
            }
          ]
        }
      ]
    }
  }
]
