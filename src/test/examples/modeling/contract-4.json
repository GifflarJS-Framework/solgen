[
  {
    "name": "DHT11",
    "contract": {
      "variables": [
        {
          "statement": "global_variable",
          "type": "address",
          "name": "manager",
          "scope": "public",
          "value": ""
        },
        {
          "statement": "global_variable",
          "type": "string",
          "name": "name",
          "scope": "public",
          "value": ""
        },
        {
          "statement": "global_variable",
          "type": "uint256",
          "name": "value1",
          "scope": "public",
          "value": ""
        },
        {
          "statement": "global_variable",
          "type": "uint256",
          "name": "max_value1",
          "scope": "public",
          "value": ""
        },
        {
          "statement": "global_variable",
          "type": "uint256",
          "name": "min_value1",
          "scope": "public",
          "value": ""
        }
      ],
      "events": [
        {
          "statement": "event",
          "name": "temperatureOverflow",
          "inputs": [
            { "name": "value1", "type": "uint256" },
            { "name": "max_value1", "type": "uint256" }
          ]
        },
        {
          "statement": "event",
          "name": "temperatureUnderflow",
          "inputs": [
            { "name": "value1", "type": "uint256" },
            { "name": "min_value1", "type": "uint256" }
          ]
        }
      ],
      "modifiers": [],
      "functions": [
        {
          "name": "",
          "scope": "public",
          "isConstructor": true,
          "inputs": [
            {
              "name": "_owner",
              "type": "address"
            }
          ],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "assignment",
              "variable": "manager",
              "value": {
                "statement": "expression",
                "value": "_owner"
              }
            },
            {
              "statement": "assignment",
              "variable": "name",
              "value": {
                "statement": "expression",
                "value": "\"DHT11\""
              }
            }
          ]
        },
        {
          "name": "setValue",
          "scope": "public",
          "isConstructor": false,
          "inputs": [
            {
              "name": "_val",
              "type": "uint256"
            },
            {
              "name": "_valueId",
              "type": "uint256"
            }
          ],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "if",
              "else": false,
              "condition": "_valueId == 1",
              "content": [
                {
                  "statement": "assignment",
                  "variable": "value1",
                  "value": {
                    "statement": "expression",
                    "value": "_val"
                  }
                },
                {
                  "statement": "if",
                  "else": false,
                  "condition": "value1 >= max_value1",
                  "content": [
                    {
                      "statement": "event_call",
                      "name": "temperatureOverflow",
                      "variables": ["value1", "max_value1"]
                    }
                  ]
                },
                {
                  "statement": "if",
                  "else": true,
                  "condition": "value1 <= min_value1",
                  "content": [
                    {
                      "statement": "event_call",
                      "name": "temperatureUnderflow",
                      "variables": ["value1", "min_value1"]
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "name": "setName",
          "scope": "public",
          "isConstructor": false,
          "inputs": [{ "name": "_name", "type": "string" }],
          "outputs": [],
          "modifiers": [],
          "content": [
            {
              "statement": "assignment",
              "variable": "name",
              "value": {
                "statement": "expression",
                "value": "_name"
              }
            }
          ]
        }
      ]
    }
  }
]
